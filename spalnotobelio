        'active-icon' : "fa-star",
        'inactive-icon' : "fa-star-o",
        'clearable' : false,
        'divclass' : 'cirating-input'
      });
    });
    if(typeof addthis == 'undefined') {
    var addthis_config = {"data_track_clickback": true};

